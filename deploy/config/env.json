{
  "verify": {
    "clientId": "$OAUTH_CLIENTID",
    "authUrl": "$OAUTH_URL",
    "scope": "$OAUTH_SCOPE",
    "response_type": "$OAUTH_RESPONSE_TYPE",
    "prompt": "$OAUTH_PROMPT",
    "login_hint": "$OAUTH_LOGIN_HINT"
  },
  "geostudio": {
    "env": "$ENV",
    "disableFT": "$DISABLE_FT",
    "disableLI": "$DISABLE_LI",
    "disableDF": "$DISABLE_DF",
    "disableFeedback": "$DISABLE_FEEDBACK",
    "useV2": true,
    "useAuthSideCar": "$USE_AUTH_SIDECAR",
    "studioApiKey": "$STUDIO_API_KEY",
    "mapboxToken": "$MAPBOX_TOKEN",
    "cesiumToken": "$CESIUM_TOKEN",
    "visibleLayersByDefault": "$VISIBLE_LAYERS_BY_DEFAULT",
    "layerOrder": "$LAYER_ORDER"
  }
}
